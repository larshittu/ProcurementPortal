
@{
    ViewBag.Title = "Create Vendor Account";
    Layout = "~/Views/Shared/_Authentication.cshtml";
}

<form class="login100-form validate-form">
    <div class="wrap-login100">

        <div class="wrap-input100 validate-input" data-validate="Valid account is required: ex@abc.xyz">
            <input class="input100" type="text" name="Account Number" placeholder="Account No">
            <span class="focus-input100"></span>
            <span class="symbol-input100">
                <i class="fa fa-money" aria-hidden="true"></i>
            </span>
        </div>

        <div class="container-login100-form-btn">
            <button class="login100-form-btn">
                Verify Account
            </button>
        </div>

        <div class="text-center p-t-12">
            @*<span class="txt1">
                Forgot
            </span>*@
            <a class="txt2" href="#">
                <a href="@Url.Action("ForgetPassword", "Account")"> Forget Password </a> OR <a href="@Url.Action("Login", "Account")"> Log In</a>
            </a>
        </div>
    </div>
</form>